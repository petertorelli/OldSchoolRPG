doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(lang='en')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		meta(name='author', content='Peter Torelli')
		link(rel='stylesheet' href='css/redist/bootstrap.min.css')
		link(rel='stylesheet' href='css/style.css')
		title Ye Olde RPG
		script(type='text/javascript' src='js/redist/jquery.min.js')
		script(type='text/javascript' src='js/redist/bootstrap.min.js')
		script(type='text/javascript' src='js/redist/three.min.js')
	body(role='document')
		ul.nav.nav-tabs#my-tabs
			li(role='presentation').active
				a(href='#level') Level Design
			li(role='presentation')
				a(href='#character') Character Generation
		.tab-content
			.tab-pane#level(role='tabpanel').active
				include level
			.tab-pane#character(role='tabpanel')
				include character
		script(type='text/javascript' src='js/level-20x20.js')
		script(type='text/javascript' src='js/level-20x20-wiz1-lvl1.js')
		script(type='text/javascript' src='js/globals.js')
		script(type='text/javascript' src='js/party.js')
		script(type='text/javascript' src='js/viewport-webgl.js')
		script(type='text/javascript' src='js/levelmap.js')
		script(type='text/javascript' src='js/index.js')
